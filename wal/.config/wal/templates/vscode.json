{{
    "workbench.colorTheme": "{wallpaper}",
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "editor.colorDecorators": false,
    "editor.cursorBlinking": "solid",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "Cascadia Code",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.links": false,
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.roundedSelection": false,
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "visible",
    "editor.selectionHighlight": false,
    "editor.showFoldingControls": "always",
    "terminal.integrated.fontFamily": "Cascadia Code",
    "terminal.integrated.fontWeight": "300",
    "workbench.editor.showIcons": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.tree.indent": 14,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorCustomizations": {{
        "activityBar.background": "{color0}",
        "breadcrumb.background": "{color8}",
        "breadcrumbPicker.background": "{color8}",
        "debugToolBar.background": "{color8}",
        "editor.background": "{background}",
        "editorBracketMatch.background": "{color0}",
        "editorGroupHeader.tabsBackground": "{background}",
        "editorHoverWidget.background": "{color8}",
        "editorOverviewRuler.border": "{color0}",
        "editorOverviewRuler.background": "{color0}",
        "editorWidget.background": "{color8}",
        "editor.lineHighlightBackground": "{color0}",
        "list.activeSelectionBackground": "{color0}",
        "list.hoverBackground": "{color8}",
        "menu.background": "{color0}",
        "notifications.background": "{color8}",
        "panel.background": "{color8}",
        "quickInput.background": "{color0}",
        "scrollbarSlider.activeBackground": "{color8}",
        "scrollbarSlider.hoverBackground": "{color8}",
        "scrollbarSlider.background": "{color8}",
        "settings.checkboxBackground": "{color8}",
        "settings.dropdownBackground": "{color8}",
        "settings.numberInputBackground": "{color8}",
        "settings.textInputBackground": "{color8}",
        "sideBar.background": "#{color0.darken(3%)}",
        "sideBar.dropBackground": "{color0}",
        "sideBarSectionHeader.background": "{color0}",
        "statusBar.background": "{color0}",
        "statusBar.noFolderBackground": "{color8}",
        "tab.activeBackground": "{color8}",
        "tab.activeBorder": "{background}",
        "tab.border": "{background}",
        "tab.inactiveBackground": "{color0}",
        "terminal.ansiBlack": "{color0}",
        "terminal.ansiBlue": "{color4}",
        "terminal.ansiBrightBlack": "{color8}",
        "terminal.ansiBrightBlue": "{color12}",
        "terminal.ansiBrightCyan": "{color14}",
        "terminal.ansiBrightGreen": "{color10}",
        "terminal.ansiBrightMagenta": "{color13}",
        "terminal.ansiBrightRed": "{color9}",
        "terminal.ansiBrightWhite": "{color15}",
        "terminal.ansiBrightYellow": "{color11}",
        "terminal.ansiCyan": "{color6}",
        "terminal.ansiGreen": "{color2}",
        "terminal.ansiMagenta": "{color5}",
        "terminal.ansiRed": "{color1}",
        "terminal.ansiWhite": "{color7}",
        "terminal.ansiYellow": "{color3}",
        "terminal.background": "{background}",
        "terminal.foreground": "{foreground}",
        "terminal.selectionBackground": "{cursor}",
        "titleBar.activeBackground": "{color8}"
    }},
    "window.menuBarVisibility": "toggle",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.symbolCacheDownload": true,
    "julia.enableTelemetry": false,
    "editor.wordWrap": "on",
    "terminal.integrated.fontSize": 16,
    "editor.lineNumbers": "relative",
    "notebook.lineNumbers": "on",
    "notebook.output.textLineLimit": 15,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.commits.avatars": false,
    "gitlens.views.commits.showBranchComparison": false,
    "gitlens.views.showRelativeDateMarkers": false,
    "window.zoomLevel": 2,
    "workbench.startupEditor": "none",
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {{
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }},
        {{
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {{
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }}
            ]
        }}
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {{
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }},
        {{
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {{
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }}
            ]
        }}
    ],
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "eq-material-theme-icons",
    // "customizeUI.activityBar": "bottom",
    "customizeUI.titleBar": "inline",
    "customizeUI.font.regular": "Roboto",
    "customizeUI.fontSizeMap": {{
        "13px": "12px",
        "monospace": "12px"
    }},
    "customizeUI.stylesheet": {{
        ".tabs-container ": "padding-top:7px; padding-left:2px; padding-bottom:5px",
        ".tabs-container > .tab": "border-radius: 7px 7px 7px 7px; height: 20px; margin-left: 5px; padding-right:4px",
        ".monaco-editor .suggest-widget": "border-radius: 10px 10px 10px 10px; overflow:hidden;"
    }},
    "editor.minimap.enabled": false,
    "makefile.makeDirectory": "0x_extra/custom_arrays",
    "makefile.makefilePath": "0x_extra/custom_arrays/Makefile",
    "Lua.telemetry.enable": false
    }}
